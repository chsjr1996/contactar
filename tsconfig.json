{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "jsx": "react",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "baseUrl": "./resources/js",
    "paths": {
      "@Root/*": ["*"],
      "@Config/*": ["Configs/*"],
      "@Interface/*": ["Interfaces/*"],
      "@Layout/*": ["Layouts/*"],
      "@Page/*": ["Pages/*"],
      "@Component/*": ["Components/*"],
      "@Style/*": ["Styles/*"],
      "@Service/*": ["Services/*"],
    }
  }
}
